{
  "name": "diplom_be",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7"
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "doctrine/annotations": "^1.7",
    "doctrine/common": "^2.9",
    "doctrine/dbal": "^2.9.2",
    "doctrine/migrations": "^2.2",
    "doctrine/orm": "^2.6.2",
    "monolog/monolog": "^2.1",
    "php-di/php-di": "^6.1",
    "slim/psr7": "^1.1",
    "slim/slim": "^4.5",
    "symfony/console": "^4.3",
    "symfony/property-access": "^5.1",
    "symfony/serializer": "4.3.*",
    "symfony/yaml": "4.3.*",
    "vlucas/phpdotenv": "^3.5"
  },
  "require-dev": {
    "jangregor/phpstan-prophecy": "^0.8.0",
    "phpstan/extension-installer": "^1.0.4",
    "phpstan/phpstan": "^0.12.25",
    "phpunit/phpunit": "^8.5"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
